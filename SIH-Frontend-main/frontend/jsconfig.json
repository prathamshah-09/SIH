{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "@components/*": ["src/components/*"],
      "@context/*": ["src/context/*"],
      "@utils/*": ["src/utils/*"],
      "@hooks/*": ["src/hooks/*"],
      "@lib/*": ["src/lib/*"],
      "@data/*": ["src/data/*"],
      "@mock/*": ["src/mock/*"],
      "@services/*": ["src/services/*"]
    }
  },
  "include": ["src"]
}